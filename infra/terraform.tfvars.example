# Example Terraform variables file
# Copy this file to terraform.tfvars and fill in your values
# DO NOT commit terraform.tfvars to version control

# GCP Project ID
project_id = "your-gcp-project-id"

# GCP Region for Cloud Run and Cloud Scheduler
region = "us-central1"

# BigQuery dataset location
bq_location = "US"

# Container image reference
# Format: <region>-docker.pkg.dev/<project>/<repository>/<image>:<tag>
image_ref = "us-central1-docker.pkg.dev/your-gcp-project-id/app-images/ingest:latest"

# Source URL for data ingestion
source_url = "https://www.ecb.europa.eu/stats/eurofxref/eurofxref-hist.csv"

